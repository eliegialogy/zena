type 'add21_title', 'add2_2'
click 'css=input[type=submit]'
wait_for_not_visible 'add21_title'
verify_text 'css=.add2_2 a', 'add2_2'
# should have set parent
verify_text 'css=.add2_2 > span', 'Zena the wild CMS'