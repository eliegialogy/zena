# zena apache2 vhost for <%= config[:host] %>
# automatically generated file

<VirtualHost *<%= vhost_port %>>
  ServerName www.<%= config[:host] %>

  RewriteEngine On
  RewriteCond %{HTTP_HOST} ^www\.<%= config[:host] %>$ [NC]
  RewriteRule ^(.*)$ http://<%= config[:host] %>$1 [R=301,L]
</VirtualHost>
